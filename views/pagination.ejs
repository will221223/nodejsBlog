<!-- 分頁module -->
<nav class="my-5" aria-label="Page navigation example">
    <ul class="pagination justify-content-center">
      <li class="page-item <% if(!page.hasPre ) { %> disabled <% }%>">
        <a class="page-link" href="/<%- page.category %>?page=<%- page.currentPage -1 %>" tabindex="-1">Previous</a>
      </li>
        <% for ( var i=1 ; i < page.pageTotal+1 ; i++) { %>
        <li class="page-item <% if(i == page.currentPage ) { %> active <% }%>"> 
          <a class="page-link" href="/<%- page.category %>?page=<%- i %>">
            <%- i %>
          </a>
        </li>
        <% } %>
        <li class="page-item <% if(!page.hasNext ) { %> disabled <% }%>">
         <a class="page-link" href="/<%- page.category %>?page=<%- page.currentPage +1 %>">Next</a>
      </li>
    </ul>
  </nav>
